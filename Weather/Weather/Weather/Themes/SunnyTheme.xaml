<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Weather.Themes.SunnyTheme">
    <Color x:Key="PageBackgroundColor">#d18213</Color>
    <Color x:Key="BarBackgroundColor">#a8690f</Color>
    <Style TargetType="NavigationPage">
        <Setter Property="BarBackgroundColor" Value="#a8690f" />
        <Setter Property="BarTextColor" Value="White" />
    </Style>
</ResourceDictionary>